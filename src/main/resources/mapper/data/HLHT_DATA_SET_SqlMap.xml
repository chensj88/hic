        </trim>
    </insert>

    <update id="updateHlhtDataSet" parameterType="com.winning.hic.model.HlhtDataSet">
        UPDATE HLHT_DATA_SET
        <set>
            <trim suffixOverrides=",">
                <if test="(id != null and id != '')"> ID , </if>
                <if test="(pId != null and pId != '')"> P_ID , </if>
                <if test="(sourceType != null and sourceType != '')"> SOURCE_TYPE , </if>
                <if test="(level != null and level != '')"> LEVEL , </if>
                <if test="(colummName != null and colummName != '')"> COLUMM_NAME , </if>
                <if test="(type != null and type != '')"> TYPE , </if>
                <if test="(dtjddm != null and dtjddm != '')"> DTJDDM , </if>
                <if test="(qrmbdm != null and qrmbdm != '')"> QRMBDM , </if>
                <if test="(qrdxdm != null and qrdxdm != '')"> QRDXDM , </if>
                <if test="(yzjddm != null and yzjddm != '')"> YZJDDM , </if>
                <if test="(bt != null and bt != '')"> BT , </if>
                <if test="(sourceName != null and sourceName != '')"> SOURCE_NAME , </if>
            </trim>
        </set>
        <where>
            <if test="(id != null and id != '')"> and ID = #{ id }   </if>
            <if test="(id !=null or id!='') and (map.pks != null and map.pks != '')">
                and (ID) in
                <foreach item="item" collection="map.pks" open="(" separator="," close=")"> #{item}</foreach>
            </if>
        </where>
    </update>

    <delete id="deleteHlhtDataSet" parameterType="com.winning.hic.model.HlhtDataSet">
        delete from  HLHT_DATA_SET
        <where>
            <if test="(id != null and id != '')"> and ID = #{ id }   </if>
            <if test="(id !=null or id!='') and (map.pks != null and map.pks != '')">
                and (ID) in
                <foreach item="item" collection="map.pks" open="(" separator="," close=")"> #{item}</foreach>
            </if>
        </where>
    </delete>


    <select id="selectHlhtDataSet" resultMap="hlhtDataSetResult" parameterType="com.winning.hic.model.HlhtDataSet">
        SELECT * FROM HLHT_DATA_SET t
        <where>
            <include refid="sql-hlhtDataSet" />
        </where>
    </select>

    <select id="selectHlhtDataSetList" resultMap="hlhtDataSetResultForList" parameterType="com.winning.hic.model.HlhtDataSet">
        SELECT
        <if test="row.count != null and row.count != ''"> top ${row.count} </if>
        * FROM HLHT_DATA_SET t
        <where>
            <include refid="sql-hlhtDataSet" />
        </where>
        ORDER BY t.ID DESC
    </select>

    <select id="selectHlhtDataSetCount" resultType="int" parameterType="com.winning.hic.model.HlhtDataSet">
        select count(1) from  HLHT_DATA_SET t
        <where>
            <include refid="sql-hlhtDataSet" />
        </where>
    </select>

    <select id="selectHlhtDataSetPageList" resultMap="hlhtDataSetResult" parameterType="com.winning.hic.model.HlhtDataSet">
        SELECT * FROM (
        SELECT t.* ,
        ROW_NUMBER() OVER ( ORDER BY t.ID DESC ) AS _RN
        FROM HLHT_DATA_SET t
        <where>
            <include refid="sql-hlhtDataSet" />
        </where>
        <!-- order by t.ID asc -->
        ) TEMP_TABLE WHERE _RN BETWEEN  #{row.first} + 1 AND #{row.first} + #{row.count}
    </select>


</mapper>