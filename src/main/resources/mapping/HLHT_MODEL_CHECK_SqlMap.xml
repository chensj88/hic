<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.winning.hic.dao.HlhtModelCheckDao">

	<resultMap id="hlhtModelCheckResultForList" type="hlhtModelCheck">
		<result column="ID" property="id" jdbcType="VARCHAR" />
		<result column="P_ID" property="pId" jdbcType="VARCHAR" />
		<result column="LEVEL" property="level" jdbcType="INTEGER" />
		<result column="QRMBDM" property="qrmbdm" jdbcType="VARCHAR" />
		<result column="COLUMM_NAME" property="colummName" jdbcType="VARCHAR" />
		<result column="TYPE" property="type" jdbcType="INTEGER" />
		<result column="DTJDDM" property="dtjddm" jdbcType="VARCHAR" />
		<result column="QRDXDM" property="qrdxdm" jdbcType="VARCHAR" />
		<result column="YZJDDM" property="yzjddm" jdbcType="VARCHAR" />
		<result column="ERROR_DESC" property="errorDesc" jdbcType="VARCHAR" />
		<result column="STATUS" property="status" jdbcType="INTEGER" />
	</resultMap>

	<resultMap id="hlhtModelCheckResult" type="hlhtModelCheck" extends="hlhtModelCheckResultForList">
	</resultMap>

	<sql id="sf-hlhtModelCheck">
	   <if test="id != null and id != ''"> and t.ID = #{id} </if>
	   <if test="pId != null and pId != ''"> and t.P_ID = #{pId} </if>
	   <if test="(level != null and level != '') or level == 0"> and t.LEVEL = #{level} </if>
	   <if test="qrmbdm != null and qrmbdm != ''"> and t.QRMBDM = #{qrmbdm} </if>
	   <if test="colummName != null and colummName != ''"> and t.COLUMM_NAME = #{colummName} </if>
	   <if test="(type != null and type != '') or type == 0"> and t.TYPE = #{type} </if>
	   <if test="dtjddm != null and dtjddm != ''"> and t.DTJDDM = #{dtjddm} </if>
	   <if test="qrdxdm != null and qrdxdm != ''"> and t.QRDXDM = #{qrdxdm} </if>
	   <if test="yzjddm != null and yzjddm != ''"> and t.YZJDDM = #{yzjddm} </if>
	   <if test="errorDesc != null and errorDesc != ''"> and t.ERROR_DESC = #{errorDesc} </if>
	   <if test="(status != null and status != '') or status == 0"> and t.STATUS = #{status} </if>
	</sql>

	<select id="selectHlhtModelCheck" resultMap="hlhtModelCheckResult" parameterType="hlhtModelCheck">
		select * from HLHT_MODEL_CHECK t where 1 = 1
		<include refid="sf-hlhtModelCheck" />
	</select>

	<select id="selectHlhtModelCheckList" resultMap="hlhtModelCheckResultForList" parameterType="hlhtModelCheck">
		select
		 <if test="row.count != null and row.count != ''"> top ${row.count} </if>
		 * from HLHT_MODEL_CHECK t where 1 = 1
		<include refid="sf-hlhtModelCheck" />
		<!-- order by ID asc -->
	</select>

	<select id="selectHlhtModelCheckCount" resultType="int" parameterType="hlhtModelCheck">
		select count(*) from HLHT_MODEL_CHECK t where 1 = 1
		<include refid="sf-hlhtModelCheck" />
	</select>

	<select id="selectHlhtModelCheckPageList" resultMap="hlhtModelCheckResult" parameterType="hlhtModelCheck">
		select * from (
			select *, ROW_NUMBER() OVER ( order by id desc ) as _rn 
				from HLHT_MODEL_CHECK t where 1 = 1
			<include refid="sf-hlhtModelCheck" />
			<!-- order by ID asc -->
		) temp_table where _rn between #{row.first} + 1 and #{row.first} + #{row.count}
	</select>

	<insert id="insertHlhtModelCheck" parameterType="hlhtModelCheck">
	<selectKey resultType="String"  order="BEFORE"  keyProperty="id">SELECT CAST(NEWID() AS VARCHAR(36)) AS ID</selectKey>
		insert into HLHT_MODEL_CHECK 
		<trim prefix="(" suffix=")" suffixOverrides=",">
		   <if test="id != null and id != ''"> ID, </if>
		   <if test="pId != null and pId != ''"> P_ID, </if>
	       <if test="(level != null and level != '') or level == 0"> LEVEL, </if>
		   <if test="qrmbdm != null and qrmbdm != ''"> QRMBDM, </if>
		   <if test="colummName != null and colummName != ''"> COLUMM_NAME, </if>
	       <if test="(type != null and type != '') or type == 0"> TYPE, </if>
		   <if test="dtjddm != null and dtjddm != ''"> DTJDDM, </if>
		   <if test="qrdxdm != null and qrdxdm != ''"> QRDXDM, </if>
		   <if test="yzjddm != null and yzjddm != ''"> YZJDDM, </if>
		   <if test="errorDesc != null and errorDesc != ''"> ERROR_DESC, </if>
	       <if test="(status != null and status != '') or status == 0"> STATUS, </if>
		</trim>
		values
		<trim prefix="(" suffix=")" suffixOverrides=",">
		   <if test="id != null and id != ''"> #{id, jdbcType=VARCHAR}, </if>
		   <if test="pId != null and pId != ''"> #{pId, jdbcType=VARCHAR}, </if>
	       <if test="(level != null and level != '') or level == 0"> #{level, jdbcType=INTEGER}, </if>
		   <if test="qrmbdm != null and qrmbdm != ''"> #{qrmbdm, jdbcType=VARCHAR}, </if>
		   <if test="colummName != null and colummName != ''"> #{colummName, jdbcType=VARCHAR}, </if>
	       <if test="(type != null and type != '') or type == 0"> #{type, jdbcType=INTEGER}, </if>
		   <if test="dtjddm != null and dtjddm != ''"> #{dtjddm, jdbcType=VARCHAR}, </if>
		   <if test="qrdxdm != null and qrdxdm != ''"> #{qrdxdm, jdbcType=VARCHAR}, </if>
		   <if test="yzjddm != null and yzjddm != ''"> #{yzjddm, jdbcType=VARCHAR}, </if>
		   <if test="errorDesc != null and errorDesc != ''"> #{errorDesc, jdbcType=VARCHAR}, </if>
	       <if test="(status != null and status != '') or status == 0"> #{status, jdbcType=INTEGER}, </if>
		</trim>
	</insert>

	<update id="updateHlhtModelCheck" parameterType="HlhtModelCheck">
		update HLHT_MODEL_CHECK
		<set>
			<trim suffixOverrides=",">
			   <if test="id != null and id != ''"> ID = #{id, jdbcType=VARCHAR}, </if>
			   <if test="pId != null and pId != ''"> P_ID = #{pId, jdbcType=VARCHAR}, </if>
			   <if test="(level != null and level != '') or level == 0"> LEVEL = #{level, jdbcType=INTEGER}, </if>
			   <if test="qrmbdm != null and qrmbdm != ''"> QRMBDM = #{qrmbdm, jdbcType=VARCHAR}, </if>
			   <if test="colummName != null and colummName != ''"> COLUMM_NAME = #{colummName, jdbcType=VARCHAR}, </if>
			   <if test="(type != null and type != '') or type == 0"> TYPE = #{type, jdbcType=INTEGER}, </if>
			   <if test="dtjddm != null and dtjddm != ''"> DTJDDM = #{dtjddm, jdbcType=VARCHAR}, </if>
			   <if test="qrdxdm != null and qrdxdm != ''"> QRDXDM = #{qrdxdm, jdbcType=VARCHAR}, </if>
			   <if test="yzjddm != null and yzjddm != ''"> YZJDDM = #{yzjddm, jdbcType=VARCHAR}, </if>
			   <if test="errorDesc != null and errorDesc != ''"> ERROR_DESC = #{errorDesc, jdbcType=VARCHAR}, </if>
			   <if test="(status != null and status != '') or status == 0"> STATUS = #{status, jdbcType=INTEGER}, </if>
			</trim>
		</set>
		where
		<if test="id != null and id != ''"> id = #{id} </if>
		<if test="(id == null or id == '') and (map.pks != null and map.pks != '')">
			ID in
			<foreach item="item" collection="map.pks" open="(" separator="," close=")">#{item}</foreach>
		</if>
	</update>

	<delete id="deleteHlhtModelCheck" parameterType="HlhtModelCheck">
		delete from HLHT_MODEL_CHECK where
		<if test="id != null and id != ''"> id = #{id} </if>
		<if test="(id == null or id == '') and (map.pks != null and map.pks != '')">
			ID in
			<foreach item="item" collection="map.pks" open="(" separator="," close=")">#{item}</foreach>
		</if>
	</delete>

</mapper>